{
  "extends": ["stylelint-config-standard"],
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "tailwind",
          "theme",
          "apply",
          "layer",
          "config",
          "import"
        ]
      }
    ],
    "function-no-unknown": null
  }
}
